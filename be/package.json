{
  "name": "watch-store-backend",
  "version": "1.0.0",
  "description": "Watch Store Pickup & Delivery Backend API",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "migrate": "flyway -configFiles=\"./flyway.conf\" migrate",
    "localstack:setup": "node src/scripts/setup-localstack-s3.js",
    "dev:full": "docker-compose up localstack -d && node src/scripts/setup-localstack-s3.js && npm run dev",
    "test": "jest --coverage"
  },
  "keywords": ["watch", "repair", "delivery"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "joi": "^17.11.0",
    "dotenv": "^16.3.1",
    "aws-sdk": "^2.1498.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1",
    "morgan": "^1.10.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}